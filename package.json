    "jest": "^19.0.2",
    "rimraf": "^2.6.1",
    "rxjs": "^5.0.1",
    "svg-url-loader": "^2.0.2",
    "ts-jest": "^19.0.10",
    "tslint": "~4.5",
    "typescript": "2.2",
    "webpack": "^2.4.1",
    "webpack-node-externals": "^1.5.4",
    "zone.js": "0.8.5"
